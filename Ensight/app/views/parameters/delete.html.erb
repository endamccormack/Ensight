<div class="parameterDelete">
	<%= link_to('< Back', {:action => 'list'}, :class => 'formBackLink') %><br/>
	<link rel="stylesheet" type="text/css" href="/css/form.css" /> 

	<div class="module">
		<div class="moduleHeader">Delete a Parameter</div>
	<%= form_for(:parameter, :url => {:action => 'destroy', :id => @parameter.id}) do |f| %>

		<%= render(:partial => "form", :locals => {:f => f}) %>


		<%= submit_tag("Confirm delete parameter", :class => "formButton ui-button ui-widget ui-state-default ui-corner-all ui-button-text-only ui-state-hover") %>

	<% end %>
	</div>


</div>