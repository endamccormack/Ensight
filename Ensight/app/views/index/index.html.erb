	<script type="text/javascript">
	
		
		function loadparameters(selobj, url, nameattr) {
			$(selobj).empty();
			$.getJSON(url, {}, function (data) {
				<!--$(selobj).append('<option value="">Select Parameter');  -->
				$.each(data, function (i, obj) {
					$(selobj).append($("<option></option>").html(obj[nameattr]));
				});
			});
		}
		
		function loadmonths(selobj, url, nameattr) {
			$(selobj).empty();
			$.getJSON(url, {}, function (data) {
				<!--$(selobj).append('<option value="">Select Month');  -->
				$.each(data, function (i, obj) {
					$(selobj).append($("<option></option>").html(obj[nameattr]));
				});
			});
		}

		$(function () {
			loadparameters($('select#parameterType').get(0), '/JsonRequest/findGetParameter', 'parameterType');
			loadmonths($('select#dateTimeTaken').get(0), '/JsonRequest/findGetMonth', 'dateTimeTaken');
			//loadmonths($('select#dateTimeTaken').get(0), 'GetMonth.php?GetMonth=dateTimeTaken', 'dateTimeTaken');
		});
		
	</script>
   
				
